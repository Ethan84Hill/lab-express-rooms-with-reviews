<h2> List of Rooms </h2>

<ul>
    {{#each foundRooms }}
        <li> <a href="/rooms/{{id}}"> {{ this.name }} </a></li>
        <p>{{description}}</p>
        <img src="{{imageUrl}}" alt="room image">

        <form action="/rooms/{{_id}}/edit-room" method="post">
            <button>Edit</button>
        </form>

        <form action="/rooms/{{_id}}/delete" method="post">
            <button>Delete</button>
        </form>
    {{/each}}
</ul>